{
  "multipart": [
    { "apply": { "model": "factorycore:block/pipe/core" }},
    { "when": { "north": "true", "extract_north": "false" }, "apply": { "model": "factorycore:block/pipe/side" }},
    { "when": { "east": "true", "extract_east": "false" }, "apply": { "model": "factorycore:block/pipe/side", "y": 90 }},
    { "when": { "south": "true", "extract_south": "false" }, "apply": { "model": "factorycore:block/pipe/side", "y": 180 }},
    { "when": { "west": "true", "extract_west": "false" }, "apply": { "model": "factorycore:block/pipe/side", "y": 270 }},
    { "when": { "up": "true", "extract_up": "false" }, "apply": { "model": "factorycore:block/pipe/side", "x": 270 }},
    { "when": { "down": "true", "extract_down": "false" }, "apply": { "model": "factorycore:block/pipe/side", "x": 90 }},
    
    { "when": { "extract_north": "true" }, "apply": { "model": "factorycore:block/pipe/extractor" }},
    { "when": { "extract_east": "true" }, "apply": { "model": "factorycore:block/pipe/extractor", "y": 90 }},
    { "when": { "extract_south": "true" }, "apply": { "model": "factorycore:block/pipe/extractor", "y": 180 }},
    { "when": { "extract_west": "true" }, "apply": { "model": "factorycore:block/pipe/extractor", "y": 270 }},
    { "when": { "extract_up": "true" }, "apply": { "model": "factorycore:block/pipe/extractor", "x": 270 }},
    { "when": { "extract_down": "true" }, "apply": { "model": "factorycore:block/pipe/extractor", "x": 90 }}
  ]
}
